{
  "name": "react-ssr",
  "version": "1.0.0",
  "scripts": {
    "build": "npm run clean && npm run build-client && npm run build-server",
    "build-client": "NODE_ENV=production BABEL_ENV=production webpack --progress --display-modules --config webpack/webpack.config.client.js",
    "build-server": "NODE_ENV=production BABEL_ENV=production webpack --progress --config webpack/webpack.config.server.js",
    "clean": "rimraf dist/ && rimraf public/app/",
    "lint": "eslint src",
    "postmerge": "npm install",
    "postrewrite": "npm install",
    "precommit": "npm run lint",
    "prepush": "npm run test",
    "start": "npm run clean & NODE_ENV=development BABEL_ENV=development babel-node src/server",
    "start-analyzer": "npm run clean & NODE_ENV=development BABEL_ENV=development NODE_ANALYZER=true babel-node src/server",
    "start-production": "NODE_ENV=production BABEL_ENV=production babel-node src/server",
    "test": "echo \"Error: no test specified\"",
    "watch-client": "webpack --watch --config webpack/webpack.config.client.js",
    "watch-server": "webpack --watch --config webpack/webpack.config.server.js"
  },
  "author": "Carlos Santana",
  "license": "MIT",
  "engines": {
    "node": "current"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "dependencies": {
    "express": "^4.15.2",
    "node-time-ago": "^1.0.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "react-router-dom": "^4.0.0",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.2.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.23.0",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-2": "^6.22.0",
    "clean-webpack-plugin": "^0.1.15",
    "compression-webpack-plugin": "^1.0.0",
    "css-loader": "^0.28.4",
    "eslint": "^4.4.1",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.2.1",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "husky": "^0.14.3",
    "isomorphic-style-loader": "^3.0.0",
    "node-sass": "^4.5.3",
    "react-hot-loader": "^3.0.0-beta.6",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "webpack": "^3.5.4",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.17.1",
    "webpack-hot-server-middleware": "0.1.0",
    "webpack-merge": "^4.0.0",
    "webpack-node-externals": "^1.5.4"
  }
}
